@model RunSmokeModel
<form asp-controller="SmokeTest" asp-action="RunSmoke" asp-antiforgery="false" method="post" class="form-horizontal" >
    <div class="modal-header">
        <button type="button" class="close"
                data-dismiss="modal" aria-hidden="true">
            &times;
        </button>
        <h4 class="modal-title">
            Add SmokeTest
        </h4>
    </div>
    <div class="modal-body">
        <input asp-for="id" type="hidden" />
        <div class="form-group">
            <label asp-for="appfile" class="col-md-4 control-label"></label>
            <div class="col-md-8">
                <input asp-for="appfile" class="form-control"  />
                <span asp-validation-for="appfile" class="text-danger"></span>
            </div>
        </div>
        <div class="form-group">
            <label asp-for="packagename" class="col-md-4 control-label"></label>
            <div class="col-md-8">
                <input asp-for="packagename" class="form-control"  />
                <span asp-validation-for="packagename" class="text-danger"></span>
            </div>
        </div>
        
    </div>
    <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
        <button type="submit" class="btn btn-primary">Submit</button>
    </div>
</form>
