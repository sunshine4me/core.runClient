@model List<TaskListModel>
@{
    ViewData["Title"] = "Job Task List";
}



<div class="row">

    <table class="table table-bordered">
        <caption>@ViewData["Title"]</caption>
        <thead>
            <tr>
                <th>JobType</th>
                <th>Name</th>
                <th>ResultType</th>
                <th>Device</th>
                <th>RunDate</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var m in Model) {
                <tr class="task" onclick="parent.location = '/Result/@m.Id';">
                    <td>@(m.JobType == 1 ? "Smoke Test" : "Unknown Type")</td>
                    <td>@m.Name</td>
                    <td>@(m.RunStatus == 0?"No Run": "Complete")</td>
                    <td>@m.Device</td>
                    <td>@m.RunDate</td>
                    
                </tr>
            }

        </tbody>
    </table>

   
</div>

